name = "pocketcasts-backup"
main = "src/index.ts"
compatibility_date = "2024-01-01"
compatibility_flags = ["nodejs_compat"]

[vars]
ENVIRONMENT = "production"

[[d1_databases]]
binding = "DB"
database_name = "pocketcasts-history"
database_id = "your-database-id-here"

[triggers]
crons = ["0 * * * *"]
